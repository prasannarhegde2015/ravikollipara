select count(*)  from WellDailyAverage
SELECT SUM(WellDailyAverage.wdaOilRateAllocated) +SUM(WellDailyAverage.wdaWaterRateAllocated) as "LiquidReate"
FROM  WellDailyAverage where wdaStartDate = '2018-07-18 18:30:00.000'
 

 SELECT SUM(WellDailyAverage.wdaOilRateAllocated) +SUM(WellDailyAverage.wdaWaterRateAllocated) as "LiquidReate"
FROM  WellDailyAverage where wdaStartDate = '2018-07-17 18:30:00.000'
